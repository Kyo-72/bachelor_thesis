[32m+[m[32mfrom z3 import *[m
[32m+[m[32mMAX_NUM_GATE = 3[m
[32m+[m
[32m+[m[32mdef nearest_neighbor(c_vec,t_vec,n,s):[m
[32m+[m[41m    [m
[32m+[m[32m    for i in range(1,n):[m
[32m+[m[32m        P = Xor(t_vec[i - 1],t_vec[i + 1])[m
[32m+[m[32m        s.add(Implies(c_vec[i],P))[m
[32m+[m
[32m+[m[32mdef one_bit_per_gate(vec,n,s):[m
[32m+[m
[32m+[m[32m    count = Int("count")[m
[32m+[m[32m    P = vec[1][m
[32m+[m[32m    for i in range(2,n):[m
[32m+[m[32m        count += vec[i][m
[32m+[m[32m    s.add(P == 1)[m
[32m+[m
[32m+[m[32mdef generate_output(f_vec,c_vec,t_vec,d,n):[m
[32m+[m[41m    [m
[32m+[m[32m    for d in range(d):[m
[32m+[m[32m        c_bit = BitVec("c_bit",n + 1)[m
[32m+[m[32m        #då€‹ç›®ã®ã‚²ãƒ¼ãƒˆ ã‚³ãƒ³ãƒˆãƒ­ãƒ¼ãƒ«ãƒ“ãƒƒãƒˆãŒã‚ã‚Œã°c_bit[m
[32m+[m[32m        for i in range(1,n + 1):[m
[32m+[m[32m            c_bit = If(c_vec[d][i],(i),c_bit)[m
[32m+[m[32m        for i in range(1,n + 1):[m
[32m+[m[32m            f_vec[i] = If(t_vec[d][i],c_bit^f_vec[i],f_vec[i])[m
[32m+[m
[32m+[m[32mdef equal_desired_output(f_vec,output_list,n,s):[m
[32m+[m[41m    [m
[32m+[m
[32m+[m[32m    for output in output_list:[m
[32m+[m[41m        [m
[32m+[m[32m        P = False[m
[32m+[m[32m        for j in range(1,n + 1):[m
[32m+[m[32m            P = Or(P,f_vec[j] == output)[m
[32m+[m
[32m+[m[32m    s.add(P)[m
[32m+[m
[32m+[m[41m    [m
[32m+[m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m
[32m+[m[32mdef calc(input_list,output_list,n):[m
[32m+[m
[32m+[m[32m    d = 1;[m
[32m+[m[32m    #ã‚²ãƒ¼ãƒˆæ•°200ã¾ã§æ¢ç´¢[m
[32m+[m
[32m+[m[32m    #è«–ç†çŠ¶æ…‹ã‚’è¡¨ç¾ã™ã‚‹å¤‰æ•°[m
[32m+[m[41m    [m
[32m+[m[32m    f_vec = [BitVec("f_vec%d" % i, n) for i in range(n + 1)][m
[32m+[m[32m    for i in range(1,n + 1):[m
[32m+[m[41m        [m
[32m+[m[32m        f_vec[i] = 0 | input_list[i][m[41m [m
[32m+[m[41m    [m
[32m+[m[32m    while(d < MAX_NUM_GATE):[m
[32m+[m[32m        gate = {}[m
[32m+[m
[32m+[m[32m        #d*n CNOTã‚²ãƒ¼ãƒˆç¾¤ã‚’è¡¨ã™å¤‰æ•°[m
