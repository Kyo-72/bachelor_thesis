[32m+[m[32mdef one_bit_per_gate(vec,n,s):[m
[32m+[m
[32m+[m[32m    P_vec = [Bool("P_vec%d" % i) for i in range(n + 1)][m
[32m+[m[41m    [m
[32m+[m
[32m+[m[32m    for i in range(1,n + 1):[m
[32m+[m[32m        P_vec[i] = vec[i][m
[32m+[m[32m        for j in range(1,n + 1):[m
[32m+[m[32m            if(i != j):[m
[32m+[m[32m                P_vec[i] = And(P_vec[i],Not(vec[j]))[m
[32m+[m[41m            [m
[32m+[m[32m    #å…¨ã¦ã®åˆ¶ç´„ã‚’orã§è¿½åŠ ã—ãŸã„[m
[32m+[m[32m    P = P_vec[1][m
[32m+[m[32m    for i in range(2,n + 1):[m
[32m+[m[32m       P = Or(P,P_vec[i])[m
[32m+[m
[32m+[m[32m    s.add(P)[m
[32m+[m[32m    #print(P)[m
[32m+[m[41m            [m
[32m+[m
[32m+[m[32mdef generate_output(f_vec,c_vec,t_vec,d,n):[m
[32m+[m[41m    [m
[32m+[m[32m    for d in range(d):[m
[32m+[m[32m        c_bit = BitVec("c_bit",n + 1)[m
[32m+[m[41m    [m
[32m+[m[32m        #dã‚³ãƒ¡ã®ã‚²ãƒ¼ãƒˆ ã‚³ãƒ³ãƒˆãƒ­ãƒ¼ãƒ«ãƒ“ãƒƒãƒˆãŒã‚ã‚Œã°c_bit[m
[32m+[m[32m        for i in range(1,n + 1):[m
[32m+[m[32m            c_bit = If(c_vec[d][i],f_vec[i]^c_bit,c_bit)[m
[32m+[m[32m        for i in range(1,n + 1):[m
[32m+[m[32m            f_vec[i] = If(t_vec[d][i],c_bit^f_vec[i],f_vec[i])[m
[32m+[m
