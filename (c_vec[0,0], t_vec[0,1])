[32m+[m[32mdef generate_random_circuit(n,d):[m
[32m+[m[32m    circuit = [["" for i in range(n)]][m
[32m+[m[32m    #depthã‚’cnotã‚²ãƒ¼ãƒˆã®æ•°ã§åˆæœŸåŒ–ï¼ˆTã‚²ãƒ¼ãƒˆãŒåŠ ã‚ã‚‹å¯èƒ½æ€§ï¼‰[m
[32m+[m[32m    gate_depth = d[m
[32m+[m[32m    for i in range(gate_depth):[m
[32m+[m[32m        gate = [" " for j in range(n)][m
[32m+[m[32m        #remoteCNOTgateã‚’ç”Ÿæˆã™ã‚‹[m
[32m+[m[32m        controll_bit = random.randrange(n)[m
[32m+[m[32m        #controll_bitã¨target_bitã§é‡è¤‡ãªã—[m
[32m+[m[32m        #NNAã‚’æº€ãŸã•ãªã„CNOTã®ã¿[m
[32m+[m[32m        tmp = random.randrange(n)[m
[32m+[m[32m        while( abs(controll_bit - tmp) < 2 ):[m
[32m+[m[32m            tmp = random.randrange(n)[m
[32m+[m[32m        target_bit = tmp[m
[32m+[m
[32m+[m
[32m+[m[32m        gate[controll_bit] = "c"[m
[32m+[m[32m        gate[target_bit] = "t"[m
[32m+[m
[32m+[m
[32m+[m[32m        circuit.append(gate)[m
[32m+[m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[32m        #Tã‚²ãƒ¼ãƒˆã‚’ç¢ºç‡ã§ç”Ÿæˆã™ã‚‹[m
[32m+[m
[32m+[m[32m        #tã‚²ãƒ¼ãƒˆã‚’ç”Ÿæˆã™ã‚‹ã‹ã®ä¹±æ•°(0ãªã‚‰ç”Ÿæˆ or æœ€å¾Œã¯å¿…ãš)[m
[32m+[m[32m        probability = random.randrange(T_GATE_PROBABILITY)[m
[32m+[m[32m        if( not probability or i == gate_depth):[m
[32m+[m[32m            gate = generate_t_gate(n)[m
[32m+[m[32m            circuit.append(gate)[m
[32m+[m[32m            #tã‚²ãƒ¼ãƒˆãŒè¿½åŠ ã•ã‚ŒãŸã®ã§ã€depthãŒå¢—åŠ [m
[32m+[m[32m            gate_depth += 1[m
[32m+[m[32m            #ã‚¤ãƒ†ãƒ¬ãƒ¼ã‚¿ãƒ¼ã‚‚å¢—åŠ [m
[32m+[m
[32m+[m[32m    return circuit[m
[32m+[m[41m            [m
[32m+[m
[32m+[m[32m"""[m
[32m+[m[32mn = 4[m
[32m+[m[32md = 15[m
[32m+[m[32mrandom_circuit = generate_random_circuit(n,d)[m
[32m+[m[32mrandom_circuit.pop(0)[m
[32m+[m
[32m+[m
[32m+[m[32mdisplay.display_circuit(random_circuit)[m
[32m+[m[32moutput_set = desired_output.desired_output([1,2,4,8],random_circuit)[m
[32m+[m[32mprint(output_set)[m
[32m+[m[32m"""[m
[32m+[m
[32m+[m
