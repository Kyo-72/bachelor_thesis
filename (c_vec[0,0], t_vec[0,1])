[32m+[m[32mdef convert_to_list(d,line):[m
[32m+[m[32m    gate_list = [" " for i in range(d)][m
[32m+[m[41m    [m
[32m+[m[32m    #ãƒ•ã‚¡ã‚¤ãƒ«ã‚’åŒºåˆ‡ã‚Šãƒªã‚¹ãƒˆã¸[m
[32m+[m[32m    gate = line.split(" ")[m
[32m+[m[32m    #ã‚²ãƒ¼ãƒˆã®ç¨®é¡[m
[32m+[m[32m    type_of_gate = gate[0][0][m
[32m+[m[32m    #ã‚²ãƒ¼ãƒˆã®å…¥å‡ºåŠ›ã®æ•°[m
[32m+[m[32m    print(type_of_gate)[m
[32m+[m[32m    num_of_io = int( gate[0][1] )[m
[32m+[m[32m    #bitæ¯ã«æ–‡å­—åˆ—ã«å¤‰æ›ã™ã‚‹[m
[32m+[m
[32m+[m[32m    #Tã‚²ãƒ¼ãƒˆ[m
[32m+[m[32m    if(type_of_gate == "T"):[m
[32m+[m[32m        for i in range(1,num_of_io):[m
[32m+[m[32m            T_gate_bit = gate[i][m
[32m+[m[32m            gate_list[ord(T_gate_bit) - ord("a")] = "T"[m
[32m+[m[41m            [m
[32m+[m[32m        T_gate_bit = gate[num_of_io][0][m
[32m+[m[32m        gate_list[ord(T_gate_bit) - ord("a")] = "T"[m[41m    [m
[32m+[m[41m        [m
[32m+[m[32m    elif(type_of_gate == "t"):[m
[32m+[m[41m        [m
[32m+[m[32m    #MCTã‚²ãƒ¼ãƒˆ[m
[32m+[m
[32m+[m[32m        #notã‚²ãƒ¼ãƒˆ[m
[32m+[m[32m        if(num_of_io == 1):[m
[32m+[m[32m            n_bit = gate[num_of_io][0][m
[32m+[m[32m            gate_list[ ord(n_bit) - ord("a")] = "n"[m
[32m+[m[32m        else:[m
[32m+[m[41m        [m
[32m+[m[32m            #ã‚³ãƒ³ãƒˆãƒ­ãƒ¼ãƒ«ãƒ“ãƒƒãƒˆã«ã¤ã„ã¦bitæ¯ã«æ–‡å­—åˆ—ã«å¤‰æ›[m
[32m+[m[32m            for i in range(1,num_of_io):[m
[32m+[m[32m                controll_bit = gate[i][m
[32m+[m[32m                gate_list[ ord(controll_bit) - ord("a")] = "c"[m
[32m+[m
[32m+[m[32m            #æœ€å¾Œã®ä¸€ãƒ“ãƒƒãƒˆã¯ã‚¿ãƒ¼ã‚²ãƒƒãƒˆãƒ“ãƒƒãƒˆ[m
[32m+[m[32m            target_bit = gate[num_of_io][0][m
[32m+[m[32m            gate_list[ ord(target_bit) - ord("a")] = "t"[m
[32m+[m
[32m+[m[32m    return gate_list[m
[32m+[m[41m    [m
[32m+[m
[32m+[m[32mdef read_file(str):[m
[32m+[m[32m    #lineã®æ•°[m
[32m+[m[32m    d = 0[m
[32m+[m[32m    flag = 0[m
[32m+[m[32m    #ãƒªã‚¹ãƒˆå½¢å¼ã§ã‚²ãƒ¼ãƒˆã‚’è¡¨ç¾[m
[32m+[m[32m    circuit = [][m
[32m+[m[41m    [m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    #ãƒ•ã‚¡ã‚¤ãƒ«ã‹ã‚‰ä¸€è¡Œãšã¤èª­ã¿è¾¼ã‚€[m
[32m+[m[32m    with open(str) as f:[m
[32m+[m[32m        for line in f:[m
[32m+[m
[32m+[m[41m            [m
[32m+[m[41m            [m
[32m+[m[32m            if(".numvars" in line):[m
[32m+[m[32m               d = int( line.split(" ")[1] )[m[41m [m
[32m+[m[32m            #beginã‹ã‚‰ã‚²ãƒ¼ãƒˆã‚’èª­ã¿å§‹ã‚ã‚‹(flag = 1)[m
[32m+[m[32m            if(".begin" in line):[m
[32m+[m[32m                flag = 1[m
[32m+[m[32m            #endãŒæ¥ãŸã‚‰èª­ã¿è¾¼ã¿çµ‚äº†[m
[32m+[m[32m            elif(".end" in line):[m
[32m+[m[32m                break[m
[32m+[m[32m            #flag = 1 ãªã‚‰ã‚²ãƒ¼ãƒˆã‚’èª­ã¿è¾¼ã‚€[m
[32m+[m[32m            elif(flag):[m
[32m+[m
[32m+[m[32m                gate_list = [][m
[32m+[m[32m                #ã‚²ãƒ¼ãƒˆã‚’ãƒªã‚¹ãƒˆã«ã™ã‚‹[m
[32m+[m[32m                gate_list = convert_to_list(d,line)[m
[32m+[m[32m                circuit.append(gate_list)[m
[32m+[m
[32m+[m[32m    print(circuit)[m
[32m+[m
[32m+[m[32mread_file("my_test1.txt")[m
[32m+[m
