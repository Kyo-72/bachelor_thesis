[32m+[m[32m.inputs 1 b c d e[m
[32m+[m[32m.outputs g g c d e[m
[32m+[m[32m.constants 1----[m
[32m+[m[32m.garbage 11---[m
[32m+[m[32m.begin[m
[32m+[m[32mt1 a[m
[32m+[m[32mt3 e b a[m
[32m+[m[32mt2 b e[m
[32m+[m[32mt4 e d c a[m
[32m+[m[32mt4 d c a e[m
